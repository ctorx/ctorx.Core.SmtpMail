<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <VersionPrefix>0.9.0.1</VersionPrefix>
    <TargetFrameworks>netstandard2;net451</TargetFrameworks>
    <AssemblyName>ctorx.Core.SmtpMail</AssemblyName>
    <PackageId>ctorx.Core.SmtpMail</PackageId>
    <Authors>Matthew Marksbury</Authors>
    <Description>Simple .NET Core SMTP Mail Librar</Description>
    <PackageTags></PackageTags>
    <PackageProjectUrl>https://github.com/ctorx/ctorx.Core.SmtpMail/</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/ctorx/ctorx.Core.SmtpMail/master/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright (c) 2017 Matthew Marksbury</Copyright>
    <Version>0.9.0.1</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyVersion>0.9.0.1</AssemblyVersion>
    <FileVersion>0.9.0.1</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Options" Version="1.1.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="NETStandard.Library" Version="2.0.0" />
  </ItemGroup>



</Project>
